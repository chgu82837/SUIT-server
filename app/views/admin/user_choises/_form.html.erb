<%= form_for @user_choise, url: @user_choise.new_record? ? admin_user_choises_path : admin_user_choise_path(@user_choise) do |f| %>
  <% if @user_choise.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user_choise.errors.count, "error") %> prohibited this user_choise from being saved:</h2>

      <ul>
      <% @user_choise.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.collection_select :user_id, User.all, :id, :email %>
  </div>
  <div class="field">
    <%= f.label :clothing_id %><br>
    <%= f.collection_select :clothing_id, Clothing.all, :id, :name %>
  </div>
  <div class="field">
    <%= f.label :liked %><br>
    <%= f.number_field :liked %>
  </div>
  <div class="field">
    <%= f.label :time %><br>
    <%= f.date_select :time %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
