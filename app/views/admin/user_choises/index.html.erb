<p id="notice"><%= notice %></p>

<h1>Listing User Choises</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Clothing</th>
      <th>Liked</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_choises.each do |user_choise| %>
      <tr>
        <td><%= user_choise.user_id %></td>
        <td><%= user_choise.clothing_id %></td>
        <td><%= user_choise.liked %></td>
        <td><%= user_choise.time %></td>
        <td><%= link_to 'Show', admin_user_choise_path(user_choise) %></td>
        <td><%= link_to 'Edit', edit_admin_user_choise_path(user_choise) %></td>
        <td><%= link_to 'Destroy', admin_user_choise_path(user_choise), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User choise', new_admin_user_choise_path %>
