<p id="notice"><%= notice %></p>

<h1>Listing Clothing Fields</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Func</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clothing_fields.each do |clothing_field| %>
      <tr>
        <td><%= clothing_field.name %></td>
        <td><%= clothing_field.func %></td>
        <td><%= link_to 'Show', admin_clothing_field_path(clothing_field) %></td>
        <td><%= link_to 'Edit', edit_admin_clothing_field_path(clothing_field) %></td>
        <td><%= link_to 'Destroy', admin_clothing_field_path(clothing_field), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clothing field', new_admin_clothing_field_path %>
